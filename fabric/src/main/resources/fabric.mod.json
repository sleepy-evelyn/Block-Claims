{
  "schemaVersion": 1,
  "id": "territorial",
  "version": "${version}",
  "name": "Territorial Block Claims",
  "description": "A wrapper for claims mods that allows you to claim, configure and sell land with simple blocks",
  "authors": [
    "SleepyEvelyn"
  ],
  "contact": {
    "sources": "https://github.com/sleepy-evelyn/Territorial-Block-Claims"
  },
  "license": "MIT",
  "icon": "assets/blockclaims/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.sleepy_evelyn.territorial.fabric.TerritorialFabric"
    ],
    "client": [
      "dev.sleepy_evelyn.territorial.fabric.client.TerritorialFabricClient"
    ],
    "fabric-datagen": [
      "dev.sleepy_evelyn.territorial.fabric.client.datagen.ClientDataGenFabric"
    ]
  },
  "mixins": [
    "territorial.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17.2",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fzzy_config": ">=0.7",
    "fabric-api": "*"
  }
}
