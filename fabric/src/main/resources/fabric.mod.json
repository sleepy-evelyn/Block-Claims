{
  "schemaVersion": 1,
  "id": "blockclaims",
  "version": "${version}",
  "name": "Block Claims",
  "description": "A wrapper for claims mods that allows you to claim, configure and sell land with simple blocks",
  "authors": [
    "SleepyEvelyn"
  ],
  "contact": {
    "sources": "https://github.com/sleepy-evelyn/Block-Claims"
  },
  "license": "MIT",
  "icon": "assets/blockclaims/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.sleepy_evelyn.blockclaims.fabric.BlockClaimsFabric"
    ],
    "client": [
      "dev.sleepy_evelyn.blockclaims.fabric.client.BlockClaimsFabricClient"
    ]
  },
  "mixins": [
    "blockclaims.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17.2",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "architectury": ">=13.0.8",
    "fabric-api": "*"
  }
}
